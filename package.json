{
  "name": "shorui",
  "version": "1.1.0",
  "description": "Simple Markdown Editing.",
  "author": "Dennis Russell",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-win": "set NODE_ENV=production&&vue-cli-service build",
    "start": "npm run serve",
    "lint": "vue-cli-service lint",
    "lint-fix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bulma": "^0.6.0",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "markdown": "^0.5.0",
    "markdown-it": "^8.4.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-ins": "^2.0.0",
    "markdown-it-mark": "^2.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-checkbox": "^1.0.4",
    "markdown-it-task-lists": "^2.0.1",
    "markdown-it-toc-and-anchor": "^4.1.2",
    "moment": "^2.19.1",
    "pouchdb-adapter-idb": "^6.3.4",
    "rxdb": "^6.0.1",
    "rxjs": "^5.5.0",
    "textics": "^0.1.1",
    "underscore.string": "^3.3.4",
    "vue": "2.6.10",
    "vue-localstorage": "^0.5.0",
    "vue-router": "^3.0.7",
    "vue-shortkey": "^3.0.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-plugin-pwa": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.2.2",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
